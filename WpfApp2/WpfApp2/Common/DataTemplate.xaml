<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:WpfApp2.ViewModel"
                    xmlns:view="clr-namespace:WpfApp2.View">

    <DataTemplate DataType="{x:Type vm:ReferenceBookViewModel}">
        <view:ReferenceBookView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MembersViewModel}">
        <view:MembersView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RaceViewModel}">
        <view:RaceView />
    </DataTemplate>

</ResourceDictionary>